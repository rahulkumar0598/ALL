@{
    ViewData["Title"] = "Event Details";
}
@model BookEventModel
<!--Details of the event-->
<div class="container">
    <h6 class="display-4">Event Details</h6>
    <div class="row">
        <div class="col-md-4">
            <partial name="" model="@Model" />
        </div>
        <div class="col-8">
            <h4>Description</h4>
            <p>@Model.EventDescription</p>
            <hr />
            <h4>Details</h4>
            <p>@Model.Otherdetails</p>
        </div>
    </div>
</div>
